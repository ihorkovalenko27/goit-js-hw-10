{{#each this}}
<div class="card">
  <div class="card-img-top">
    <img width="30px" height="30" src="{{flag}}" alt="{{name}}">
  </div>
  <div class="card-body">
    <h2 class="card-title">Name: {{name}}</h2>
    <p class="card-text">Capital: {{capital}}</p>
    <p class="card-text">Population: {{population}}</p>

    
    <ul class="list-group"></ul>
    {{#each languages}}
      <li class="list-group-item">Languages: {{this.name}}</li>
    {{/each}}
    </ul>
  </div>
</div>
{{/each}}